{
  "version": "0.5.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "hooks",
    "webchemist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WebchemistCorp/hooks"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@webchemist/hooks",
  "author": "GennYoon",
  "module": "dist/hooks.esm.js",
  "size-limit": [
    {
      "path": "dist/hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "diff": "6.0.0-beta",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "https-proxy-agent": "^7.0.5",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.1.0",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recast": "^0.23.9",
    "ts-morph": "^23.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/diff": "^5.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.template": "^4.5.3",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "size-limit": "^11.0.2",
    "tsdx": "^0.13.3",
    "tslib": "^2.6.2",
    "turbo": "^2.1.0",
    "type-fest": "^4.25.0",
    "typescript": "^3.9.10"
  }
}
