{
  "version": "0.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WebchemistCorp/hooks"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@webchemist/hooks",
  "author": "GennYoon",
  "module": "dist/hooks.esm.js",
  "size-limit": [
    {
      "path": "dist/hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "husky": "^9.0.6",
    "jest": "^25.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^3.9.10"
  }
}
